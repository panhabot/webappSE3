<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServ</servlet-name>
        <servlet-class>LoginServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServ</servlet-name>
        <url-pattern>/LoginServ</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SuccuessServ</servlet-name>
        <servlet-class>SuccessServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SuccuessServ</servlet-name>
        <url-pattern>/SuccessServ</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>HomeServ</servlet-name>
        <servlet-class>HomeServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeServ</servlet-name>
        <url-pattern>/HomeServ</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>AboutServ</servlet-name>
    <servlet-class>AboutServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AboutServ</servlet-name>
        <url-pattern>/AboutServ</url-pattern>
    </servlet-mapping>
    <servlet>

    <servlet-name>ContactServ</servlet-name>
    <servlet-class>ContactServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContactServ</servlet-name>
        <url-pattern>/ContactServ</url-pattern>
    </servlet-mapping>
    <servlet>

        <servlet-name>ProfileServ</servlet-name>
        <servlet-class>ProfileServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileServ</servlet-name>
        <url-pattern>/ProfileServ</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>filter</filter-name>
        <filter-class>filter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>filter</filter-name>
        <url-pattern>/SuccessServ</url-pattern>
        <url-pattern>/HomeServ</url-pattern>
        <url-pattern>/AboutServ</url-pattern>
        <url-pattern>/ContactServ</url-pattern>
        <url-pattern>/ProfileServ</url-pattern>

    </filter-mapping>
</web-app>